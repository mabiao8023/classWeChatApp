@import "./../components/contact.wxss";
page{background:#eee}.container{background:#eee;padding-top:80rpx}.nav{position:fixed;top:0;display:flex;width:100%;border-bottom:2rpx solid #eaeaea;background:#fff}.nav .nav-item{width:25%;text-align:center;line-height:80rpx;color:#333;font-size:30rpx}.nav .nav-item.active{color:#FF741F;border-bottom:4rpx solid #FF741F}.game-list{width:100%}.game-item{display:flex;align-items:center;width:100%;padding:24rpx;background:linear-gradient(to bottom, #fff 0, #F7F6F5 100%);border-bottom:1px solid #CBCBCB}.game-item .game-item-sc{width:100rpx}.game-item .game-item-content{flex:1}.game-item .game-item-con-info{display:flex;font-size:28rpx;margin-bottom:10rpx}.game-item .game-item-con-info .game-ls{width:40%;text-align:right}.game-item .game-item-con-info .game-time{margin:0 20rpx;color:#999;width:80rpx;text-align:center}.game-item .game-item-con-info .game-minutes{color:#FF741F}.game-item .game-name{display:flex;font-size:30rpx}.game-item .home{display:flex;width:40%;justify-content:flex-end;align-items:center}.game-item .other{display:flex}.game-item .score{margin:0 20rpx;width:80rpx;text-align:center;color:#FF741F;font-size:32rpx}.game-item .red{background:red;color:#fff;padding:2rpx 5rpx;border-radius:6rpx;overflow:hidden;margin:0 5rpx}.game-item .yellow{background:yellow;color:#333;padding:2rpx 5rpx;border-radius:6rpx;overflow:hidden;margin:0 5rpx}.collect{width:60rpx;height:55rpx;vertical-align:middle}
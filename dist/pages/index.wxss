@import "./../components/contact.wxss";
page{background:#eee;padding-top:80rpx}.container{background:#eee}.nav{position:fixed;top:0;display:flex;width:100%;border-bottom:2rpx solid #eaeaea;background:#fff}.nav .nav-item{width:25%;text-align:center;line-height:80rpx;color:#333;font-size:30rpx}.nav .nav-item.active{color:#FF741F;border-bottom:4rpx solid #FF741F}.game-list{width:100%}.game-item{display:flex;align-items:center;width:100%;padding:30rpx 24rpx;background:linear-gradient(to bottom, #fff 0, #F7F6F5 100%);border-bottom:1px solid #CBCBCB}.game-item .game-item-sc{width:48rpx;margin-right:20rpx}.game-item .game-item-content{flex:1}.game-item .game-item-con-info{display:flex;font-size:28rpx;margin-bottom:10rpx}.game-item .game-item-con-info .game-ls{width:38%;text-align:left;color:#999;display:flex;justify-content:flex-end}.game-item .game-item-con-info .game-time{margin-left:20rpx;color:#999;width:80rpx;text-align:right}.game-item .game-item-con-info .game-minutes{margin:0 20rpx;width:60rpx;text-align:center;color:#FF741F}.game-item .game-name{display:flex;font-size:30rpx;color:#333}.game-item .home{display:flex;width:38%;justify-content:flex-end;align-items:center}.game-item .other{display:flex;align-items:center}.game-item .score{margin:0 20rpx;width:60rpx;text-align:center;color:#FF741F;font-size:32rpx}.game-item .red{background:#e41c36;color:#fff;width:26rpx;height:32rpx;line-height:32rpx;border-radius:6rpx;text-align:center;overflow:hidden;margin:0 5rpx;font-size:26rpx}.game-item .yellow{background:#f6c151;color:#fff;width:26rpx;height:32rpx;line-height:32rpx;border-radius:6rpx;text-align:center;overflow:hidden;font-size:26rpx;margin:0 5rpx}.collect{width:48rpx;height:48rpx;vertical-align:middle}.share{width:40rpx;height:40rpx}
.contact-container{position:fixed;left:0;right:0;bottom:0;z-index:1000;background:#FF741F;transition:left .1s linear}.contact-container.left0{left:0}.contact-container image{margin-top:12rpx;margin-right:12rpx;width:40rpx;height:40rpx;vertical-align:top}.gz-contact{display:inline-block;width:100%;text-align:center;line-height:80rpx;font-size:32rpx;color:#fff;padding-right:8rpx;padding-left:10rpx;background:transparent;border:transparent;outline:0;border-radius:0;padding:0;box-shadow:0}.gz-contact::after{content:"";display:none}
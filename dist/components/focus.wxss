.contact-container{position:fixed;left:-90rpx;height:68rpx;bottom:160rpx;z-index:1000;border-bottom-right-radius:50rpx;border-top-right-radius:50rpx;background:#FF741F;transition:left .1s linear}.contact-container.left0{left:0}.contact-container image{margin-top:12rpx;margin-right:12rpx;width:40rpx;height:40rpx;vertical-align:top}.gz-contact{display:inline-block;width:100rpx;line-height:68rpx;font-size:30rpx;color:#fff;padding-right:8rpx;padding-left:10rpx;background:transparent;border:transparent;outline:0;border-radius:0;padding:0;box-shadow:0}.gz-contact::after{content:"";display:none}
.contact {
  position: fixed;
  right: 0;
  z-index: 1000;
  line-hegiht: 50rpx;
  bottom: 60rpx;
  font-size: 32rpx;
  border-radius: 0;
  color: #fff;
  padding-right: 10rpx;
  padding-left: 20rpx;
  border-bottom-left-radius: 50rpx;
  border-top-left-radius: 50rpx;
  background: #2cbd6c;
  box-shadow: 0 2rpx 10rpx rgba(0, 0, 0, 0.5);
  border-right: 0;
}
.contact image {
  margin-top: 15rpx;
  width: 48rpx;
  height: 48rpx;
  vertical-align: top;
}
.contact.share {
  bottom: 100rpx;
}
